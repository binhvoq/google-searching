# Copy file này thành terraform.tfvars và điền thông tin của bạn

# Azure Subscription ID (bắt buộc)
subscription_id = "your-subscription-id-here"

# Azure Region (mặc định: eastus)
location = "eastus"

# Tên Resource Group (để trống sẽ tự động tạo)
# resource_group_name = "rg-my-aifoundry"

# Tên Azure AI Foundry (để trống sẽ tự động tạo)
# ai_foundry_name = "my-aifoundry"

# Model configuration
model_name    = "gpt-4o-mini"
model_version = "2024-07-18"

# Deployment SKU và Capacity
deployment_sku      = "GlobalStandard"
deployment_capacity = 1

# Tags
tags = {
  Environment = "dev"
  Project     = "ai-foundry-demo"
  ManagedBy   = "Terraform"
}

